<?xml version="1.0"?>
<sdf version='1.8'>
  <world name="not_so_simple_trees">
    <!-- Basic world setup  -->
    <plugin name='ignition::gazebo::systems::Physics' filename='ignition-gazebo-physics-system' />
    <plugin name='ignition::gazebo::systems::UserCommands' filename='ignition-gazebo-user-commands-system' />
    <plugin name='ignition::gazebo::systems::SceneBroadcaster' filename='ignition-gazebo-scene-broadcaster-system' />
    <plugin name='ignition::gazebo::systems::Contact' filename='ignition-gazebo-contact-system' />
    <light name="sun" type="directional">
      <cast_shadows>1</cast_shadows>
      <pose>0 0 10 0 -0 0</pose>
      <diffuse>0.8 0.8 0.8 1</diffuse>
      <specular>0.2 0.2 0.2 1</specular>
      <attenuation>
        <range>1000</range>
        <constant>0.9</constant>
        <linear>0.01</linear>
        <quadratic>0.001</quadratic>
      </attenuation>
      <direction>-0.5 0.1 -0.9</direction>
    </light>
    <gravity>0 0 -9.81</gravity>
    <magnetic_field>6e-06 2.3e-05 -4.2e-05</magnetic_field>
    <atmosphere type="adiabatic"/>
    <physics name="default_physics" type="ignored">
      <max_step_size>0.01</max_step_size>
      <real_time_factor>1</real_time_factor>
      <real_time_update_rate>100</real_time_update_rate>
    </physics>
    <scene>
      <ambient>1.0 1.0 1.0</ambient>
      <background>0.8 0.8 0.8</background>
      <shadows>1</shadows>
    </scene>

    <!-- Initialise spherical coordinates somewhere -->
    <spherical_coordinates>
      <latitude_deg>0.0</latitude_deg>
      <longitude_deg>0.0</longitude_deg>
      <elevation>10.0</elevation>
      <heading_deg>0</heading_deg>
      <surface_model>EARTH_WGS84</surface_model>
    </spherical_coordinates>

    <!-- Small ground plane with grass texture -->
    <include>
      <uri>
        model://grass_plane
      </uri>
      <name>grass_plane</name>
      <pose>0 0 0 0 0 0</pose>
    </include>

    <!-- Oak tree -->
    <include>
      <uri>
        https://fuel.gazebosim.org/1.0/OpenRobotics/models/Oak%20tree
      </uri>
      <name>oak1</name>
      <pose>0 3.0 0 0 0 0</pose>
    </include>
    
    <!-- Pine tree -->
    <include>
      <uri>
        https://fuel.gazebosim.org/1.0/OpenRobotics/models/Pine%20tree
      </uri>
      <name>pine1</name>
      <pose>5.0 0.0 0 0 0 0</pose>
    </include>
    
    <!-- Small ground plane with grass texture -->
    <include>
      <uri>
        model://crop_rows
      </uri>
      <name>crop_rows</name>
      <pose>30 30 0 0 0 0.7854</pose>
    </include>

    <include>
      <uri>
        model://muscle_car
      </uri>
      <name>muscle_car</name>
      <pose>0 0 0 0 0 0</pose>
    </include>

    <include>
      <uri>
        model://muscle_car
      </uri>
      <name>muscle_car_2</name>
      <pose>-10 -10 0 0 0 0</pose>
    </include>

    <include>
      <uri>
        model://farm_house
      </uri>
      <name>farm_house</name>
      <pose>-30 -30 0 0 0 0</pose>
    </include>

    <include>
      <uri>
        model://farm_barn
      </uri>
      <name>farm_barn</name>
      <pose>-30 0 0 0 0 0</pose>
    </include>

    <include>
      <uri>
        model://shipping_container
      </uri>
      <name>shipping_container</name>
      <pose>50 10 0 0 0 1.2</pose>
    </include>

    <include>
      <uri>
        model://rock1
      </uri>
      <name>rock1</name>
      <pose>30 25 0 0 0 0</pose>
    </include>

    <include>
      <uri>
        model://rock2
      </uri>
      <name>rock2</name>
      <pose>10 10 0 0 0 0</pose>
    </include>

    <include>
      <uri>
        model://tractor_new
      </uri>
      <name>tractor_new</name>
      <pose>-10 10 0 0 0 0</pose>
    </include>

    <include>
      <uri>
        model://silo
      </uri>
      <name>silo</name>
      <pose>50 50 0 0 0 0</pose>
    </include>

    <include>
      <uri>
        model://silo
      </uri>
      <name>silo</name>
      <pose>50 50 0 0 0 0</pose>
    </include>

  </world>
</sdf>
